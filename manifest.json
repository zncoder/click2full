{
  "name": "click2full",
  "version": "1.0.0",
  "manifest_version": 2,
  "description": "Click toolbar button to trigger fullscreen",
  "permissions": [
    "activeTab"
  ],
  "icons": {
    "48": "icons/48.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": "icons/32.png",
    "default_title": "Click to go fullscreen"
  }
}
